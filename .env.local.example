# Supabase
NEXT_PUBLIC_SUPABASE_URL=
SUPABASE_SERVICE_ROLE_KEY=

# BlurPay Contract (заполнить после деплоя: npm run deploy:sepolia или deploy:base)
NEXT_PUBLIC_BLUR_PAY_ADDRESS=

# Для деплоя: приватный ключ кошелька (никогда не коммитить!)
# DEPLOYER_PRIVATE_KEY=

# Base RPC (для base / baseSepolia)
BASE_RPC_URL=https://mainnet.base.org

# Ethereum Sepolia RPC (только если NEXT_PUBLIC_CHAIN=sepolia)
# SEPOLIA_RPC_URL=https://rpc.sepolia.org

# Сеть: base | baseSepolia | sepolia (Ethereum Sepolia)
NEXT_PUBLIC_CHAIN=base

# App URL (for Frame metadata)
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Farcaster: accountAssociation (JSON) — опционально
# FARCASTER_ACCOUNT_ASSOCIATION=
